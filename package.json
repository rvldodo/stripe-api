{
	"name": "api-typescript",
	"version": "1.0.0",
	"description": "create API using typescript and yarn",
	"main": "server.js",
	"author": "Rivaldo Ardika Lawalata",
	"license": "MIT",
	"scripts": {
		"dev": "cross-env process.env.NODE_ENV='Development' nodemon ./src/server.ts",
		"build": "cross-env process.env.NODE_ENV='Production' node ./build/server.js",
		"m:migrate": "sequelize db:migrate",
		"s:latest": "sequelize db:seed:all",
		"db:sync": "sequelize db:drop && sequelize db:create --charset utf8 && npm run m:migrate && npm run s:latest"
	},
	"dependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/body-parser": "^1.19.2",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.15.5",
		"@types/sequelize": "^4.28.14",
		"@types/swagger-ui-express": "^4.1.3",
		"bcrypt": "^5.1.0",
		"body-parser": "^1.20.2",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.0",
		"morgan": "^1.10.0",
		"pg": "^8.10.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.29.3",
		"swagger-ui-express": "^4.6.2",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.2"
	}
}
